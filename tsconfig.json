{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "nativewind-env.d.ts",
    "app/DevSection/utilities/backup.tsx"
  ],
  "exclude": [
    "app/DevSection/utilities/backup.tsx" // Exclude your temporary file
  ]
}
